{
  "extends": [
    ":semanticCommits"
  ],
  "ignorePaths": [
    ".github/workflows/build.yml"
  ],
  "labels": [
    "maintenance"
  ],
  "lockFileMaintenance": {
    "enabled": true
  },
  "packageRules": [
    {
      "matchPaths": [
        "package.json",
        "yarn.lock"
      ],
      "semanticCommitType": "fix"
    }
  ],
  "semanticCommitScope": null
}